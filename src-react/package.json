{
  "name": "just-open",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build --outDir ../src-py/gui --emptyOutDir --minify && cd ../src-py && uv run pyinstaller build-windows.spec",
    "lint": "eslint .",
    "preview": "vite preview",
    "py": "cd ../src-py && uv run main.py --verbose",
    "models": "cd ../src-py && uv run pydantic2ts --module apps/models.py --output ../src-react/src/types/models.d.ts"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.0",
    "@reduxjs/toolkit": "^2.9.2",
    "@szhsin/react-menu": "^4.5.0",
    "classnames": "^2.5.1",
    "lodash": "^4.17.21",
    "natsort": "^2.0.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.38.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.4.0",
    "mime-types": "^3.0.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.12",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
